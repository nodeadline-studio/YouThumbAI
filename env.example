# YouThumbAI Environment Configuration
# Copy this file to .env and add your actual API keys

# OpenAI Configuration (Required for DALL-E 3 generation)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# YouTube API Configuration (Required for video analysis)
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here

# Replicate Configuration (Required for face swap and LoRA models)
VITE_REPLICATE_API_TOKEN=your_replicate_api_token_here

# Hugging Face Configuration (Optional for LoRA models)
VITE_HUGGINGFACE_API_TOKEN=your_huggingface_token_here

# Development Configuration
NODE_ENV=development
VITE_API_BASE_URL=http://localhost:3001
VITE_ENV=development
VITE_DEBUG=true

# CORS Server Configuration
CORS_PORT=3001 